{
    "name": "yourturn-dev-apo",
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "env": {
      "MONGODB_URI": "@my-mongodb-uri"
    },
    "routes": [
      {
        "src": "/users/(.*)",
        "dest": "api/index.js",
        "methods": ["GET", "POST","PATCH", "DELETE"]
      },
      {
        "src": "/tasks/(.*)",
        "dest": "api/index.js",
        "methods": ["GET", "POST","PATCH", "DELETE"]
      },
      {
        "src": "/house/(.*)",
        "dest": "api/index.js",
        "methods": ["GET", "POST","PATCH"]
      }
    ]
  }